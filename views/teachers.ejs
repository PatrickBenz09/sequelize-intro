<html>
<head>
  <title>Hiksuu</title>
</head>
<body>
  <table border="1 px solid black">
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
    <% datas.forEach(data => { %>
    <tr>
      <th><%= data.first_name %></th>
      <th><%= data.last_name %></th>
      <th><%= data.email %></th>
    </th>
    <% }) %>
  </table>
</body>
</html>
